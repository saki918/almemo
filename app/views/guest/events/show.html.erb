<h1>Guest::Events#show</h1>
<p>Find me in app/views/guest/events/show.html.erb</p>
<p><%= notice %></p>
  <%= "ジャンル" %>
  <%= @event.genre_id %><br>
  <%= "タイトル" %>
  <%= @event.title %><br>
  <%= "開始時間" %>
  <%= @event.start_time %><br>
  <%= "終了時間" %>
  <%= @event.finish_time %><br>
  <%= "コメント" %>
  <%= @event.comment %><br>
  <%= "投稿画像" %>
  <% @images each do |image| %>
  <%= attachment_image_tag :image %><br>

  <%= link_to "編集", edit_event_path(@event), class: "btn btn-success" %>
  <%= link_to "削除", event_path(@event), method: :delete, class: "btn btn-danger","data-confirm" => "削除しますか？" %>