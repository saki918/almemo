<h1>ジャンル一覧</h1>
<div>
<%= link_to "ジャンル新規作成", new_admin_genre_path, class:"btn btn-success" %>
</div><br>
<% @genres.each do |genre| %>
<div class = "col-md-2 col-xs-3" >
  <%= genre.name %><br>
  <%= image_tag genre.icon_id.to_s, size:10*10 %>
  <%= link_to "削除", admin_genre_path(genre), method: :delete, class: "btn btn-danger","data-confirm" => "削除しますか？" %>
</div>
<% end %>

